@ViewBag.searchLocationMessage
@{ 
    var loc = ViewBag.searchLocationResult;
}
<h3>@loc.Name</h3>
<h4>Region:</h4><p>@loc.Region</p>
<h4>Start Coordinates:</h4><p>@loc.Start</p>
<h4>End Coordinates:</h4><p>@loc.End</p>
<h4>Peak Elevation:</h4><p>@loc.FinalEv</p>
<h4>Elevation Gain:</h4><p>@loc.EvGain</p>
<h4>Distance</h4><p>@loc.Distance</p>
<h4>Physical Difficulty</h4><p>@loc.PDiff</p>
<h4>Techincal Difficulty</h4><p>@loc.TDiff</p>
<h4>Description</h4><p>@loc.Description</p>